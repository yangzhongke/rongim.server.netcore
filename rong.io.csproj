<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard1.3</TargetFramework>
    <AssemblyName>rong.io</AssemblyName>
    <Description>RongIM .Net Core Server SDK http://rong.io</Description>
    <Authors>yangzhongke</Authors>
    <PackageProjectUrl>https://github.com/yangzhongke/rongim.server.netcore</PackageProjectUrl>
    <RepositoryUrl>https://github.com/yangzhongke/rongim.server.netcore</RepositoryUrl>
    <PackageId>rongim.server.netcore</PackageId>
    <Product>rongIM</Product>
    <GeneratePackageOnBuild>True</GeneratePackageOnBuild>
    <Version>1.0.4</Version>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="newtonsoft.json" Version="13.0.1" />
    <PackageReference Include="System.Text.Encodings.Web" Version="4.5.1" />
  </ItemGroup>

</Project>